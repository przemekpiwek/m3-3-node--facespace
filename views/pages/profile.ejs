<%- include('../partials/header') %>
<div class="profile-page">
    <div class="profile-page--banner "></div>
  <div class="profile-page__user">
      <img src=<%-user.avatarUrl%> class="profile-page__user--avatar" alt="User Profile Picture">
      <h2><%-user.name%></h2>
    </div>
    <div class="profile-page__friends">
        <h3><%- user.name%>'s Friends</h3>
    <ul class="profile-page__friends--list">
        <%friends.forEach(friend =>{%>
            <li class="friend ">
            <img src="<%-friend.avatarUrl%>" alt="Friend's Profile Picture">
            <p><%-friend.name%></p>
        </li>
        <%})%>
    </ul>
  </div>
</div>
<%- include('../partials/footer') %>